{
  "version": "0.2.8",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "prepublish": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ]
  },
  "name": "@highoutput/hds-date-picker",
  "author": "jadekenneth.github.io",
  "module": "dist/hds-date-picker.esm.js",
  "size-limit": [
    {
      "path": "dist/hds-date-picker.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hds-date-picker.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@floating-ui/react": "^0.22.3",
    "date-fns": "^2.29.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "uuid": "9.0.0"
  }
}
