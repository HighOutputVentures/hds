{
  "version": "0.2.3-alpha.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "buildEntries": "--entry find ./src -name 'index*.tsx'",
    "build": "tsdx build --entry ./src/index.tsx --entry ./src/components/Accordion/index.tsx",
    "test": "tsdx test --passWithNoTests",
    "prepare": "tsdx build --entry ./src/index.tsx --entry ./src/components/Accordion/index.tsx  --transpileOnly",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate:types": "chakra-cli tokens ./src/theme/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "name": "@highoutput/hds",
  "author": {
    "name": "Niel Berongoy"
  },
  "module": "dist/hds.esm.js",
  "size-limit": [
    {
      "path": "dist/hds.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hds.esm.js",
      "limit": "10 KB"
    }
  ],
  "exports": {
    ".": "./dist/index.js",
    "./accordion": {
      "production": {
        "import": "./dist/components/Accordion/index.esm.production.min.js",
        "require": "./dist/components/Accordion/index.cjs.production.min.js"
      },
      "development": {
        "import": "./dist/components/Accordion/index.esm.development.min.js",
        "require": "./dist/components/Accordion/index.cjs.development.min.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "accordion": [
        "dist/components/Accordion/index.d.ts"
      ],
      "alert": [
        "dist/components/Alert/index.d.ts"
      ],
      "button": [
        "dist/components/Button/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.8",
    "@swc/helpers": "^0.4.12",
    "size-limit": "7.0.8"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "2cc842169e828ff56ef57e039534fb0414854443",
  "dependencies": {
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.4.4",
    "@chakra-ui/styled-system": "^2.3.4",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@highoutput/hds-icons": "^0.1.32",
    "framer-motion": "^7.10.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
