{
  "version": "0.1.6",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --transpileOnly",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@highoutput/hds-empty-state",
  "author": "jadekenneth.github.io",
  "module": "dist/hds-empty-state.esm.js",
  "size-limit": [
    {
      "path": "dist/hds-empty-state.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hds-empty-state.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.1",
    "husky": "^8.0.3",
    "size-limit": "^8.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@highoutput/hds-icons": "*"
  }
}
