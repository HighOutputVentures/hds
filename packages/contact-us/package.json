{
  "version": "0.2.24",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "prepublish": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate:types": "chakra-cli tokens ./src/theme/index.ts"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@highoutput/hds-contact-us",
  "author": {
    "name": "Niel Berongoy"
  },
  "module": "dist/hds-contact-us.esm.js",
  "size-limit": [
    {
      "path": "dist/hds-contact-us.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hds-contact-us.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.8",
    "size-limit": "7.0.8"
  },
  "dependencies": {
    "@highoutput/hds": "^0.2.23",
    "@highoutput/hds-forms": "^0.2.6",
    "@highoutput/hds-icons": "^0.1.37"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.[tj]sx?$": "babel-jest",
      "^.+\\.mdx?$": "@storybook/addon-docs/jest-transform-mdx"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@storybook/addon-docs))"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "fdd8e792c3efa26acd6026d3e90b5eacd97cd5b4"
}
